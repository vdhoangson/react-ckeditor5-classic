    "prop-types": "^15.5.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^11.0.1",
